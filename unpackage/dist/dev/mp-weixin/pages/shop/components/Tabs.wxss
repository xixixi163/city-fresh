
.tabs {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 20rpx;
}
.tabs text {
	display: block;
	height: 60rpx;
	margin-right: 60rpx;
	line-height: 60rpx;
	font-size: 33rpx;
	color: #666666;
}
.active {
	color: #333;
	font-weight: 700;
	border-bottom: 5rpx solid #77cd69;
}

